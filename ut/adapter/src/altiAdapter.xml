                        <option>
                            <description></description>
                            <image></image>
                            <text>YES</text>
                            <value>1</value>
                        </option>
                    </optionList>
                </choiceParameter>
                <choiceParameter>
                    <name>other_altibase_skip_delete</name>
                    <description>Do you want to enable the OTHER_ALTIBASE_SKIP_DELETE property?</description>
                    <explanation></explanation>
                    <value></value>
